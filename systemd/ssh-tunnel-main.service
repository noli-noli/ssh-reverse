[Unit]
Description=SSH Tunnel Service
After=network.target

[Service]
User=「user」
ExecStart=/usr/bin/autossh -N -M 0 -o ServerAliveInterval=60 -o ServerAliveCountMax=3 -R 49201:127.0.0.1:22 -i /home/「key user」/.ssh/test user@「IP Address」
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target